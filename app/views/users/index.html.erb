<h1>Userのindexページ</h1>
<h2>Users</h2>
<thead>
        <tr>
          <th>image</th>
          <th>name</th>
        </tr>
      </thead>
  <% @users.each do |user| %>
    <table class="table">
      <tbody>
        <tr>
         <td><%= user.profile_image_id %></td>
         <td><%= user.name %></td>
         <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
      </tbody>
    </table>
  <% end %>